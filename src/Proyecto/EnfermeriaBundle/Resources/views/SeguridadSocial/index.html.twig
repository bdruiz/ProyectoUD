{% extends ('layout.html.twig') %}

{% block body -%}
    <h1>SeguridadSocial list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Eps</th>
                <th>Rh</th>
                <th>Telefonocontacto</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('ud_enfermeria_ssocial_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.eps }}</td>
                <td>{{ entity.rh }}</td>
                <td>{{ entity.telefonoContacto }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('ud_enfermeria_ssocial_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ud_enfermeria_ssocial_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('ud_enfermeria_ssocial_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
